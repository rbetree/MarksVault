{
  "name": "marksvault",
  "version": "0.1.1",
  "description": "智能管理、整理和安全备份您的书签数据",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "dev:edge": "wxt -b edge",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "build:edge": "wxt build -b edge",
    "build:all": "npm run build && npm run build:firefox && npm run build:edge",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "zip:edge": "wxt zip -b edge",
    "zip:all": "npm run zip && npm run zip:firefox && npm run zip:edge",
    "clean": "wxt clean",
    "prepare": "wxt prepare",
    "postinstall": "wxt prepare",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\" \"wxt.config.ts\"",
    "test": "jest"
  },
  "author": "",
  "license": "AGPL-3.0",
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.237",
    "@types/jest": "^29.5.1",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@wxt-dev/module-react": "^1.1.0",
    "eslint": "^8.40.0",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.5.0",
    "wxt": "^0.20.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}
